{
   "arrowParens": "avoid",
   "bracketSpacing": true,
   "endOfLine": "lf",
   "htmlWhitespaceSensitivity": "css",
   "insertPragma": false,
   "jsxBracketSameLine": false,
   "jsxSingleQuote": false,
   "printWidth": 100,
   "quoteProps": "as-needed",
   "requirePragma": false,
   "semi": true,
   "singleQuote": true,
   "tabWidth": 3,
   "trailingComma": "all",
   "useTabs": false,
   "overrides": [
      {
         "files": ".prettierrc",
         "options": { "parser": "json" }
      }
   ]
}
